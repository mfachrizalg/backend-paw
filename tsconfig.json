{
  "compilerOptions": {
      "module": "ESNext",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "target": "ESNext",
      "noImplicitAny": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "dist",
      "strict": true,
      "baseUrl": ".",
      "skipDefaultLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "paths": {
          "*": ["node_modules/*", "src/types/*"]
      }
  },
  "include": ["./src/**/*"]
  }